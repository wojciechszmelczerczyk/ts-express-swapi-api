generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}


model Film {
  id Int @id @default(autoincrement())
  title String @unique
  release_date String
  characters String[]
  filmList FilmList @relation(fields: [listId], references: [id])
  listId Int
}

model FilmList {
  id    Int      @id @default(autoincrement())
  name  String   @unique
  films Film[]
}
